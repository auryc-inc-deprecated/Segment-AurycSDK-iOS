{
  "name": "AurycSDK",
  "version": "1.1.6",
  "summary": "A short description of Auryc.",
  "description": "Long description of AurycSDK",
  "homepage": "https://auryc.com",
  "license": "Commercial",
  "authors": {
    "Alcides Zelaya": "alcides.zelaya@auryc.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "path": "."
  },
  "source_files": "AurycSDK/**/*.{h,m,swift,xcassets}",
  "exclude_files": "AurycSDK/Info.plist",
  "resources": [
    "AurycSDK/AurycSDKPrivate/Feedback.storyboard",
    "AurycSDK/AurycSDKPrivate/*.xib"
  ],
  "resource_bundles": {
    "AurycSDKFeedbackImages": [
      "AurycSDK/AurycSDKFeedbackImages/*.png"
    ]
  },
  "dependencies": {
    "CocoaLumberjack": [

    ],
    "AFNetworking": [

    ],
    "JWT": [

    ],
    "Protobuf": [
      "3.8"
    ],
    "DeviceUtil": [
      "6.1.0"
    ],
    "Base64": [

    ],
    "CocoaAsyncSocket": [
      "7.6.3"
    ],
    "SocketRocket": [

    ],
    "DLRadioButton": [
      "1.4.12"
    ],
    "IQKeyboardManager": [
      "6.5.4"
    ],
    "KVOController": [
      "1.2.0"
    ]
  },
  "static_framework": true,
  "pod_target_xcconfig": {
    "ALWAYS_SEARCH_USER_PATHS": "YES"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-lObjC"
  },
  "requires_arc": true
}
